{
	"name": "wundershine",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build:env": "sh ./scripts/build-env.sh",
		"build:e2e": "detox build --configuration $E2E_CONFIG",
		"build:e2e:ios": "E2E_CONFIG=ios.sim.release yarn build:e2e",
		"build:e2e:android": "E2E_CONFIG=android.emu.release yarn build:e2e",
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"launch:ios": "react-native run-ios --simulator='iPhone X'",
		"launch:android": "react-native run-android",
		"test": "jest --passWithNoTests",
		"test:e2e": "detox test --configuration $E2E_CONFIG",
		"test:e2e:ios": "E2E_CONFIG=ios.sim.release yarn test:e2e",
		"test:e2e:android": "E2E_CONFIG=android.emu.release yarn test:e2e",
		"lint": "eslint src __tests__ --fix",
		"precommit": "npm run lint",
		"prepush": "npm run test",
		"prepare": "patch-package"
	},
	"eslintIgnore": [
		"config.env.js"
	],
	"dependencies": {
		"axios": "^0.18.0",
		"cloudinary-core": "^2.5.0",
		"mobx": "4.0.2",
		"mobx-persist": "0.4.1",
		"mobx-react": "5.0.0",
		"native-base": "^2.3.10",
		"p-min-delay": "^2.0.0",
		"prop-types": "15.6.1",
		"qs": "^6.5.1",
		"react": "16.3.0-alpha.1",
		"react-native": "0.54.2",
		"react-native-background-upload": "https://github.com/Wundershine/react-native-background-upload",
		"react-native-blur": "3.2.2",
		"react-native-code-push": "5.3.1",
		"react-native-config": "0.11.5",
		"react-native-easy-grid": "^0.1.17",
		"react-native-fetch-blob": "^0.10.8",
		"react-native-interactable": "0.1.10",
		"react-native-joi": "^0.0.5",
		"react-native-keyboard-aware-scroll-view": "^0.5.0",
		"react-native-linear-gradient": "^2.4.0",
		"react-native-loading-placeholder": "^0.0.6",
		"react-native-navigation": "1.1.407",
		"react-native-path": "^0.0.5",
		"react-native-sentry": "0.35.2",
		"react-native-share-extension": "https://github.com/Wundershine/react-native-share-extension",
		"react-native-typography": "^1.3.0",
		"react-native-ui-lib": "3.3.32",
		"react-native-vector-icons": "^4.5.0",
		"tslib": "1.9.0",
		"uuid": "^3.2.1"
	},
	"devDependencies": {
		"babel-core": "6.26.0",
		"babel-eslint": "8.2.2",
		"babel-jest": "22.4.1",
		"babel-plugin-import-glob": "^2.0.0",
		"babel-plugin-module-resolver": "3.1.0",
		"babel-preset-react-native": "4.0.0",
		"babel-preset-react-native-stage-0": "1.0.1",
		"detox": "7.2.0",
		"enzyme": "3.3.0",
		"enzyme-adapter-react-16": "1.1.1",
		"eslint": "4.18.2",
		"eslint-config-airbnb": "16.1.0",
		"eslint-import-resolver-babel-module": "4.0.0",
		"eslint-plugin-detox": "1.0.0",
		"eslint-plugin-import": "2.9.0",
		"eslint-plugin-jest": "21.14.1",
		"eslint-plugin-jsx-a11y": "6.0.3",
		"eslint-plugin-react": "7.7.0",
		"eslint-plugin-react-native": "3.2.1",
		"husky": "0.14.3",
		"jest": "22.4.2",
		"lint-staged": "7.0.0",
		"mocha": "5.0.4",
		"patch-package": "5.1.1",
		"postinstall-prepare": "1.0.1",
		"react-dom": "16.3.0-alpha.1",
		"react-native-rename": "2.2.2",
		"react-test-renderer": "16.3.0-alpha.1",
		"ts-jest": "22.4.1",
		"tslint": "5.9.1"
	},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"preset": "react-native",
		"modulePaths": [
			"<rootDir>"
		],
		"setupFiles": [
			"./scripts/setup-jest.js"
		],
		"testMatch": [
			"**/__tests__/**/*.spec.js"
		]
	},
	"detox": {
		"test-runner": "mocha",
		"specs": "__tests__",
		"runner-config": "__tests__/mocha.opts",
		"configurations": {
			"ios.sim.release": {
				"binaryPath": "ios/build/Build/Products/Release-iphonesimulator/Wundershine.app",
				"build": "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -workspace ios/Wundershine.xcworkspace -scheme Wundershine -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -quiet",
				"type": "ios.simulator",
				"name": "iPhone 7 Plus"
			},
			"ios.sim.debug": {
				"binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Wundershine.app",
				"build": "xcodebuild -workspace ios/Wundershine.xcworkspace -scheme Wundershine -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -quiet",
				"type": "ios.simulator",
				"name": "iPhone 7 Plus"
			},
			"android.emu.release": {
				"binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
				"build": "pushd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && popd",
				"type": "android.emulator",
				"name": "Nexus_5_API_27"
			},
			"android.emu.debug": {
				"binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
				"build": "pushd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && popd",
				"type": "android.emulator",
				"name": "Nexus_5_API_27"
			}
		}
	}
}
